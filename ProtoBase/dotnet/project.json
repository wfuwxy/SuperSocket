{
    "name": "SuperSocket.ProtoBase",
    "version": "1.7.0.7",
    "authors": [ "Kerry Jiang", "Other contributors" ],
    "buildOptions": {
        "emitEntryPoint": false,
        "compile": {
            "include": [ "../**/*.cs" ],
            "exclude": [ "../dotnet/**/*.cs" ],
            "includeFiles": [ "../../Solution Items/GlobalAssemblyInfo.cs" ],
            "excludeFiles": [ "../Properties/AssemblyInfo.Portable.cs", "../LINQ.cs", "../Func.cs" ]
        }
    },

    "frameworks": {
        "netstandard1.1": {
            "buildOptions": {
                "define": [ "NETSTANDARD1_1" ]
            },
            "dependencies": {
                "System.Linq": "4.1.0",
                "System.Runtime.Extensions": "4.1.0",
                "System.Text.Encoding": "4.0.11",
                "System.IO": "4.1.0",
                "System.Threading": "4.0.11"
            }
        }
    },
    
    "packOptions": {
        "summary": "It is a network protocol resolving library which can be used in both server and client",
        "tags": [ "SuperSocket", "Network Protocol", "Protocol Analysis" ],
        "owners": [ "Kerry Jiang" ],
        "releaseNotes": "First release for .net core",
        "projectUrl": "http://www.supersocket.net/",
        "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
        "requireLicenseAcceptance": true,
        "repository": {
            "type": "git",
            "url": "https://github.com/kerryjiang/SuperSocket/tree/master/ProtoBase"
        }
    }
}
